{  "$schema": "https://json-schema.org/draft/2020-12/schema",  "title": "Unity Telemetry Schema",  "type": "object",  "required": ["session_id", "user_id", "timestamp", "events"],  "properties": {    "session_id": { "type": "string" },    "user_id": { "type": "string" },    "timestamp": { "type": "string", "format": "date-time" },    "context": {      "type": "object",      "properties": {        "skill_id": { "type": "string" },        "level": { "type": "string" },        "environment": { "type": "string" },        "slope_percent": { "type": "number" },        "surface": { "type": "string" }      }    },    "events": {      "type": "array",      "items": {        "type": "object",        "required": ["t", "type"],        "properties": {          "t": { "type": "number", "description": "seconds since session start" },          "type": { "type": "string", "description": "telemetry or user action type" },          "data": { "type": "object" }        }      }    }  },  "description": "Event types: posture(lean_deg_fwd/back), speed(mps), wheel_angle(deg), hand_pos(clock_angle), stroke(forceN), contact(collision), tip_warning(front/rear), pop_height(in), ramp_grade(%), turn_radius(m), stop_distance(m), etc."}